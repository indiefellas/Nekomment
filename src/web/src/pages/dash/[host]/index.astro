---
import Host from "../../../components/dashboard/Host.astro";
import MainLayout from "../../../layouts/MainLayout.astro";
import lodash from "lodash";

const { env } = Astro.locals.runtime;
const { host } = Astro.params;
const page = parseInt(Astro.url.searchParams.get("page") || "1", 10) - 1;

const user = await env.NEKOMMENT_API.checkUser(Astro.cookies.get('token')?.value ?? '')
---

<MainLayout>
    <Host user={user} host={host} />
</MainLayout>
